<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">

    <mapped-superclass name="Vankosoft\PaymentBundle\Model\ExchangeRateService">
        <id name="id" column="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        
        <field name="title" type="string" length="255" />
        <field name="serviceId" column="service_id" type="string" length="255" />
        <field name="options" type="json" nullable="true" />
        
    </mapped-superclass>
</doctrine-mapping>